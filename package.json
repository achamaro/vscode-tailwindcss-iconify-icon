{
  "name": "tailwindcss-iconify-icon-intellisense",
  "publisher": "achamaro",
  "displayName": "TailwindCSS Iconify Icon IntelliSense",
  "description": "This extension supports @achamaro/tailwindcss-iconify-icon",
  "keywords": [
    "tailwindcss",
    "iconify",
    "icon",
    "achamaro/tailwindcss-iconify-icon"
  ],
  "icon": "./icon.png",
  "version": "0.0.2",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/achamaro/vscode-tailwindcss-iconify-icon.git"
  },
  "engines": {
    "vscode": "^1.79.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage"
  ],
  "contributes": {
    "configuration": [
      {
        "id": "TailwindCSSIconifyIconIntelliSense",
        "title": "TailwindCSS Iconify Icon IntelliSense",
        "properties": {
          "tailwindcssIconifyIconIntelliSense.downloadDir": {
            "type": "string",
            "default": "src/assets/icons",
            "description": "downloadDir",
            "scope": "resource"
          },
          "tailwindcssIconifyIconIntelliSense.customSvg": {
            "type": "object",
            "default": {},
            "description": "customSvg",
            "scope": "resource"
          },
          "tailwindcssIconifyIconIntelliSense.targetLanguage": {
            "type": "array",
            "default": [
              "typescript",
              "javascript",
              "javascriptreact",
              "typescriptreact",
              "html",
              "vue",
              "vue-html",
              "css"
            ],
            "description": "target language",
            "scope": "window"
          }
        }
      }
    ]
  },
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "dependencies": {
    "@achamaro/tailwindcss-iconify-icon": "^0.1.2",
    "glob": "^10.2.7",
    "minimatch": "^9.0.1",
    "throttle-debounce": "^5.0.0"
  },
  "devDependencies": {
    "@types/minimatch": "^5.1.2",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/throttle-debounce": "^5.0.0",
    "@types/vscode": "^1.79.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vscode/test-electron": "^2.3.2",
    "@vscode/vsce": "^2.19.0",
    "eslint": "^8.41.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "mocha": "^10.2.0",
    "prettier": "^2.8.8",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.1.3"
  }
}
